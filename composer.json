{
  "name": "solidwork/contao-slider-captcha-bundle",
  "type": "contao-bundle",
  "description": "Slider CAPTCHA Formularfeld f\u00fcr Contao 4.13 und 5.3.",
  "keywords": [
    "contao",
    "captcha",
    "form",
    "slider"
  ],
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": ">=7.4",
    "contao/core-bundle": ">=4.13 || >=5.3"
  },
  "autoload": {
    "psr-4": {
      "Solidwork\\ContaoSliderCaptchaBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Solidwork\\ContaoSliderCaptchaBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true
    }
  }
}